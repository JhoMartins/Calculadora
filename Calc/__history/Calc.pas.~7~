unit Calc;

interface

type TCalc = class
public
      function Adicao(x,y : integer):integer;
      function Subtracao (x,y : integer):integer;
      procedure ovelha;
end;

implementation


{ TCalc }

function TCalc.Adicao(x, y: integer): integer;
begin
   Result:= x + y;
end;

procedure TCalc.ovelha;
begin

end;

function TCalc.Subtracao(x, y: integer): integer;
begin
  Result:= x - y;
end;

end.
